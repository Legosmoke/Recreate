local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("MICRO HUB 1.0.1b  |  PRIVATE", "DarkTheme")


--MAIN
local Tab = Window:NewTab("Combat")
local MainSection = Tab:NewSection("Things")


MainSection:NewButton("TEXTURE PACK", "yes", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/eLeCtRaDoMiNuS/newtexture/main/yes.lua", true))()
end)


MainSection:NewButton("meteor texture pack", "new", function()
    local obj = game:GetObjects("rbxassetid://11144793662")[1]
    obj.Name = "Part"
    local connection
    connection = game:GetService("ReplicatedStorage").ChildAdded:Connect(function(v)
	 for i,x in pairs(obj:GetChildren()) do
		 local c = x:Clone()
		 c.Parent = v
	    end
	   connection:Disconnect()
        end)
   loadstring(game:HttpGet("https://raw.githubusercontent.com/ExanlesLuashit/Vape-CustomModules/main/Astral-Minecraft-TexturePack"))()			
end)
